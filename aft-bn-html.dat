use aft-html.dat
ID	HTML

Preamble{
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
<title>%title%</title>
	<meta name="author" content="%author%">
	<meta name="GENERATOR" content = "%version%">
<style type="text/css">
